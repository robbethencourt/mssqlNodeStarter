{
  "name": "mssqlNodeStarter",
  "version": "0.0.0",
  "description": "starter for node and ms sql",
  "main": "server",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "//": "start server",
    "flow": "flow check || exit 0",
    "clientLint": "eslint src/**/*.js js/**/*.jsx webpack.config.js -- --fix && standard src/**/*.js",
    "clientTest": "jest",
    "clientTestClearCashe": "jest -- -no-cashe",
    "clientCoverage": "npm run clientTest -- --coverage",
    "serverLint": "eslint server/**/*.js -- --fix && standard server/**/*.js",
    "serverTest": "mocha server/test",
    "prebuild:dev": "rimraf dist/*",
    "dev": "webpack-dev-server --env.dev",
    "build:dev": "webpack --env.dev",
    "prebuild": "rimraf dist/*",
    "build": "webpack -p --env.prod",
    "start": "nodemon server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robbethencourt/mssqlNodeStarter.git"
  },
  "author": "Rob Bethencourt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robbethencourt/mssqlNodeStarter/issues"
  },
  "homepage": "https://github.com/robbethencourt/mssqlNodeStarter",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      ".scss$": "<rootDir>/src/__mocks__/styleMock.js"
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  },
  "dependencies": {
    "axios": "^0.15.3",
    "body-parser": "~1.16.1",
    "express": "~4.14.1",
    "morgan": "~1.8.1",
    "mssql": "^3.3.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "4.0.0-alpha.5"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "css-loader": "^0.27.3",
    "enzyme": "^2.8.0",
    "enzyme-to-json": "^1.5.0",
    "eslint": "^3.18.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-config-standard-flow": "^1.0.1",
    "eslint-config-standard-jsx": "^3.3.0",
    "eslint-config-standard-react": "^4.3.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^2.1.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.42.0",
    "html-webpack-plugin": "^2.28.0",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "jasmine-core": "^2.5.2",
    "jest": "^19.0.2",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.1",
    "progress-bar-webpack-plugin": "^1.9.3",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "standard": "~8.6.0",
    "style-loader": "^0.16.0",
    "webpack": "^2.3.1",
    "webpack-config-utils": "^2.3.0",
    "webpack-dev-server": "^2.4.2"
  },
  "engines": {
    "node": "6.5.0"
  }
}
