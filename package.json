{
  "name": "mssqlNodeStarter",
  "version": "0.0.0",
  "description": "starter for node and ms sql",
  "main": "server",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prebuild": "rimraf dist/bundle.js",
    "build": "webpack -p --env.prod",
    "prebuild:dev": "rimraf dist/bundle.js",
    "dev": "webpack-dev-server --env.dev",
    "build:dev": "webpack --env.dev",
    "test": "standard --fix && standard && mocha",
    "karmaTest": "karma start",
    "watch:test": "npm karmaTest -- --auto-watch --no-single-run",
    "start": "node server/server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robbethencourt/mssqlNodeStarter.git"
  },
  "author": "Rob Bethencourt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robbethencourt/mssqlNodeStarter/issues"
  },
  "homepage": "https://github.com/robbethencourt/mssqlNodeStarter",
  "dependencies": {
    "body-parser": "~1.16.1",
    "express": "~4.14.1",
    "morgan": "~1.8.1",
    "mssql": "^3.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "css-loader": "^0.27.3",
    "karma": "^1.5.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "standard": "~8.6.0",
    "style-loader": "^0.16.0",
    "webpack": "^2.3.1",
    "webpack-config-utils": "^2.3.0",
    "webpack-dev-server": "^2.4.2",
    "webpack-validator": "^2.3.0"
  },
  "engines": {
    "node": "6.5.0"
  }
}
